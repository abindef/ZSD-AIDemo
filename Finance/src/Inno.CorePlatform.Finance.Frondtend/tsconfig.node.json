  "include": ["vite.config.*", "vitest.config.*", "cypress.config.*", "playwright.config.*","plugins/*.*"],
  "compilerOptions": {
    "lib": ["esnext", "dom"],
    "moduleResolution": "bundler",
    "composite": true,
